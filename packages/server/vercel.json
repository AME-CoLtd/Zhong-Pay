{
  "version": 2,
  "name": "zhong-pay-server",
  "builds": [
    {
      "src": "src/adapter/vercel.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "src/adapter/vercel.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "src/adapter/vercel.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
